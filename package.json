{
  "name": "@xingyuchen/mysql-mcp-server",
  "version": "4.0.3",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "guangxiang-mysql-mcp": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "README_ENHANCED.md",
    "SMITHERY_DEPLOY.md",
    "smithery.yaml"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc && node -e \"try{require('fs').chmodSync('dist/index.js', '755')}catch(e){}\"",
    "dev": "ts-node --esm src/index.ts",
    "start": "node dist/index.js",
    "install-global": "npm run build && npm install -g .",
    "quick-setup": "npm install && npm run build && echo \"✅ 安装完成！请查看 README.md 了解配置方法\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "mysql",
    "database",
    "model-context-protocol",
    "ai-assistant",
    "vscode",
    "cline",
    "sql",
    "crud",
    "transaction"
  ],
  "author": {
    "name": "guangxiangdebizi",
    "url": "https://github.com/guangxiangdebizi"
  },
  "license": "MIT",
  "description": "全新架构的MySQL数据库MCP服务器v4.0，支持StreamableHTTP协议、Header预配置、AI动态连接管理、多数据库切换，专为AI助手设计",
  "homepage": "https://github.com/guangxiangdebizi/MySQL_MCP#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/guangxiangdebizi/MySQL_MCP.git"
  },
  "bugs": {
    "url": "https://github.com/guangxiangdebizi/MySQL_MCP/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "mysql2": "^3.6.5",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "supergateway": "^2.8.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
